/* pages/note/note.wxss */
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  padding:50rpx 0;
}
.page-body {
  width: 80%;
  height: 100vh;
  background: #438ACE;
  /* box-shadow: 0px 0px 5px 5px #F17252; */
  color: #fff;
  font-size: 26rpx;
}

.foot{
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: flex-end;
}
.text-box text{
  padding-bottom: 20rpx;
  letter-spacing: 4rpx;
}
.foor text{
  padding-bottom: 10rpx;
  letter-spacing: 4rpx;
}