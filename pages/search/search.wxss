/**rpx*/
.nav_top {
  height: 12vh;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.back_logo{
  width: 40rpx;
  height: 40rpx;
}
.back_logo_box{
  display: flex;
  width: 15vw;
  justify-content: flex-end;
  align-items: center;
}
.search_box{
  height: 5vh;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #f5f5f5;
  border-radius: 5%;
  width: 80vw;
}
.s_icon{
  height: 2.5vh;
  width: 2.5vh;
  margin-left: 4vw;
}
.s_input{
  margin-left: 2vw;
  font-size: 30rpx;
  line-height: 3vh;
  height: 3vh;
  color: #979997;

}
.phcolor{
   color:#cbcdcc;
}
.s_button{
  background: #fff;
  color: #438ACE;
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 20rpx;
    font-size: 30rpx;
}
.s_button:active{
  background: #A2B4BA;
}
.userinfo{
    margin-bottom: 20rpx;
   width: 80vw;
   display: flex;
   flex-direction: row;
   align-items: center;
   justify-content: space-between;
   border-radius: 10rpx;
   margin: 2vh 0 2vh 0;
   color: #000;
   font-size: 30rpx;
}
.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.search_bd{
  height: 80vh;
  width: 80vw;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #AFB3AF;
}
.no_found{
  padding-bottom: 5vh;
  text-align: center;
}
.back_image{
    width:60vw;
    height:44vw;
}
.search_tips{
     width: 560rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  color: #87CEEB;
  font-size: 28rpx;
}
.tips{
  height: 5vh;
    color: #fff;
    font-size: 25rpx;
     display: flex;
    flex-direction: column;
    align-items: center;
    flex-wrap:wrap;
    justify-content:center;
}
.sub_btn{
  display: flex;
  height: 50rpx;
  width: 100rpx;
  border-radius: 10%;
  color: #AFB3AF;
  font-size: 20rpx;
  line-height: 50rpx;
  margin: 0;
  align-items: center;
  justify-content: center;
}
.left_view{
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.userinfo-text{
    display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 3vw;
}
.userinfo-text-name{
  font-size: 30rpx;
  color: #646664;
}
.userinfo-text-motto{
  font-size: 22rpx;
  color: #AFB3AF;
}