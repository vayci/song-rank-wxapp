.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: flex-start;
  font-size: 32rpx;
  padding:50rpx 0;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page-body {
  width: 80%;
  height: 100vh;
  background: #438ACE;
  /* box-shadow: 0px 0px 5px 5px #F17252; */
